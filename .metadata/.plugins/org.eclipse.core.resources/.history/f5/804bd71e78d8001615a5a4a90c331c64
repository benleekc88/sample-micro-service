spring.application.name=sample-service
server.port=${PORT:2020}
eureka.client.register-with-eureka=true
eureka.client.service-url.defaultZone=http://localhost:8761/eureka

spring.cloud.stream.bindings.worksChannel.binder=rabbit
spring.cloud.stream.bindings.worksChannel.destination=work.exchange
spring.cloud.stream.bindings.worksChannel.contentType=application/json
spring.cloud.stream.bindings.worksChannel.group=testgroup