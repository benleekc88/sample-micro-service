spring:
  application:
    name: work-dispatcher

server:
  port: 8080

spring:
  cloud:
    stream:
      bindings:
        worksChannel:
          binder: rabbit
          destination: work.exchange
          contentType: application/json
          group: testgroup

eureka:
  client:
    register-with-eureka: true
    service-url:
      defaultZone: http://localhost:8761/eureka
